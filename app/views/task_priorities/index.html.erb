<h1>Listing Task Priorities</h1>

<table class="list">
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% for task_priority in @task_priorities %>
  <tr>
    <td><%= task_priority.name %></td>
    <td><%= task_priority.description %></td>
    <td><%= show_link task_priority %></td>
    <td><%= edit_link edit_task_priority_path(task_priority) %></td>
    <td><%= destroy_link task_priority %></td>
  </tr>
<% end %>
</table>

<br />

<%= new_link 'task priority', new_task_priority_path %>
